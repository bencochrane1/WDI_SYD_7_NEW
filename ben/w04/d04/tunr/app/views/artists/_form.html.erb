<%= form_for @artist do |f| %>
  <div class="form_field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form_field">
    <%= f.label :hometown %>
    <%= f.text_field :hometown %>
  </div>
  <div class="form_field">
    <%= f.label :bio %>
    <%= f.text_area :bio %>
  </div>
  <div class="form_field">
    <%= f.label :years_active %>
    <%= f.number_field :years_active %>
  </div>
  <div class="form_field">
    <%= f.label :label %>
    <%= f.text_field :label %>
  </div>
  <div class="form_action">
    <%= f.submit %>
  </div>
<% end %>
